
- content_for :left_menu do
  &nbsp;

- content_for :right_menu do
  &nbsp;

- content_for :right do
  .head-1
    %h1= t('cities.list')

  = render 'features', :feature_cities => @feature_cities

- content_for :left do
  .all-cities
    .all-cities-2
      #cities_index_canvas
      %ul
        - @cities.each do |city|
          %li
            = link_to city['name_'+@locale.to_s], city_path(city.cityname)
            = render 'cities/meta', :city => city
